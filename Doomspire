--Functions

Open.MouseButton1Click:Connect(function()
Gui.Visible = not Gui.Visible
if Gui.Visible == true then
   Open.Text = "Close"
elseif Gui.Visible == false then
   Open.Text = "Open"
end
end)

Green.MouseButton1Click:Connect(function()
game.Players.LocalPlayer.Character.HumanoidRootPart.CFrame = CFrame.new(-1.042715072631836, 23.600013732910156, -121.39115142822266)
end)

